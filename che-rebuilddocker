#!/bin/bash

SCRIPT_DIR=$( dirname "${0}" )
# shellcheck source=env.sh
. "${SCRIPT_DIR}/env.sh"

TAG=$( getCurrentBranch )

sh "${SCRIPT_DIR}"/che-buildq assembly

pushd "${CHEDIR}"/dockerfiles/che || exit
sh -x build.sh --organization:quay.io/mvala --prefix:che --name:server --tag:${TAG} --dockerfile:Dockerfile
popd || exit

echo "docker push quay.io/mvala/che-server:${TAG}"
